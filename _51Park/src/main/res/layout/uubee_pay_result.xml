<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include
        layout="@layout/uubee_activity_title"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_pay_result">
        <ImageView
            android:id="@+id/iv_pay_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="2dp"
            android:src="@drawable/img_pay_failed"/>
    </FrameLayout>
    <TextView
        android:id="@+id/tv_pay_info"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/gray_939393"
        android:textSize="16sp"/>
    <Button
        android:id="@+id/return_button"
        android:layout_width="match_parent"
        android:layout_height="38dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/uubee_btn_bg_normal"
        android:text="@string/return_back"
        android:textColor="@android:color/white"
        android:textSize="15sp"/>
</LinearLayout><!-- From: file:/E:/workspace/PrepayDemo/prepay/src/main/res/layout/activity_pay_result.xml -->